<script>
$(document).ready(function(){
	$('#hidden-div-instructor').hide();
	$('#hidden-div-student').hide();
	$('#hidden-div-producer').hide();
	$('#hidden-div-login').hide();
	$('#main-root').show();
});
</script>
 
 <ul class="tab-all tabs" style="margin-top: 0; margin-bottom: 0;">
 	<li class="tab-links login-button">LOGIN</li>
	<li class="tab-links main-root-button">MAIN</li>
 </ul>

<script>
	$('.login-button').on('click', function(){
		$('#hidden-div-login').show();
		$('#hidden-div-instructor').hide();
		$('#hidden-div-producer').hide();
		$('#hidden-div-student').hide();
		$('#main-root').hide();
	});

	$('.main-root-button').on('click', function(){
		$('#hidden-div-login').hide();
		$('#hidden-div-instructor').hide();
		$('#hidden-div-producer').hide();
		$('#hidden-div-student').hide();
		$('#main-root').show();
	});
</script>

