<div id="hidden-div-student" class="tab-content">
<%= notice %>
<h1>SIGNUP</h1>
<p>Welcome on Board Student!<br>
Thanks for using your key. <br>
Sign Up here to start enjoying all the benefits that our GA portal offers.</p>

<%= form_for @student, url:{:controller=>'students', :action=>'create', :id => @student.id} do |f| %>
  	<%= f.text_field :name, :placeholder => "Name" %>
	<%= f.email_field :email, :placeholder => "Email" %>
	<%= f.text_field :cohort_id, :placeholder => "Cohort" %>
	<%= f.password_field :password, :placeholder => "Password" %>
	<%= f.password_field :password_confirmation, :placeholder => "Confirm Password" %>
	<%= f.submit "Submit" %>
<% end %>
</div>