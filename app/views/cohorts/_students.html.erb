<div id="cohort-students">
  <h1>Students</h1>
  <table class="class">
    <tr>
      <td>Student</td>
      <td>Completion Rate</td>
      <td>Pending Assignments</td>
    </tr>
    <tr>
      <% @students.each do |student| %>
        <td><%=link_to student.name, student_path(student) %></td>
        <td><%= student.completion_rate %>%</td>
        <td><%= student.pending_assignments? %></td>
      <% end %>
    </tr>
  </table>
</div>