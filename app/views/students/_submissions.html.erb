<div id="submissions">
<h2>Submissions</h2>

<table>
  <tr>
    <th>Date Submitted</th>
    <th>Title</th>
    <th>Link to Your Homework</th>
    <th>Grade</th>
  </tr>
  <% @submissions.each do |submission| %>
    <tr>
      <td><%= submission.submitted_on %></td>
      <td><%= link_to submission.title, student_submission_path(@student.id, submission) %></td>
      <td><a href="<%= submission.submission_link %>">Click Here</a></td>
      <td><%= submission.grade_status %> </td>
    </tr>
  <% end %>
</table>
</div>
