<div id="gists-students">
<h1>Homework Gists</h1>

<table class="table">
  <tr>
    <th>Date Posted</th>
    <th>Title</th>
    <th>Link</th>
  </tr>

  <% @gists.each do |gist| %>
    <tr>
      <td><%= gist.date %></td>
      <td><%= link_to gist.title, gist_path(gist) %></a></td>
      <td><a href="<%= gist.gist_link %>">Click Here</a></td>
    </tr>
  <% end %>
</table>
</div>