<h1><%= @student.name %></h1>

<ul>
  <% @submissions.each do |submission| %>
    <li>
      <%= submission.submitted_on %>  <%= submission.assignment.title %>  <%= submission.grade_status %>
    </li>
  <% end %>
</ul>

<%= form_for @submission do |f| %>
  <%= f.text_field :comments %>
<% end %>

<ul>
  <%= @gists.each do |gist| %>
    <li>
      <%= gist.title %>  <%= gist.gist_link %>
    </li>
  <% end %>
</ul>